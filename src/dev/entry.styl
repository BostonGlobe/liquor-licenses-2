$miller = 'Miller', Georgia, Times New Roman, Times, serif
$benton = 'Benton', Helvetica, Arial, sans-serif
$serif = 'Georgia', Times New Roman, Times, serif
$sans-serif = 'Helvetica', Arial, sans-serif


.test-class
	border-bottom 1px
	
#map
	width 100%
	height 700px
	

.info {
    padding: 6px 8px;
    font: 14px/16px Arial, Helvetica, sans-serif;
    background: white;
    background: rgba(255,255,255,0.8);
    box-shadow: 0 0 15px rgba(0,0,0,0.2);
    border-radius: 5px;
}
.info h4 {
    margin: 0 0 5px;
    color: #777;
}

.legend { text-align: left; line-height: 18px; color: #555; } 
.legend i { width: 18px; height: 18px; float: left; margin-right: 8px; opacity: 0.7; }


	.graphichead {font-family:'Benton', Helvetica, Arial, sans-serif; font-size:1.2em;font-weight:bold; display:block;}
	.subtitle {font-family:'Benton', Helvetica, Arial, sans-serif; font-size:0.9em; line-height:1.4em; margin-top:1em; margin-bottom:1em;}

@media only screen and (max-width: 600px) {
	.graphichead {font-family:'BentonSans', Helvetica, Arial, sans-serif; font-weight:bold;}
	.value {font-family:'BentonSans', Helvetica, Arial, sans-serif; font-weight:bold;}
}